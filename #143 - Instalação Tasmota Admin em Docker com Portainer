services:
  tasmoadmin:
    container_name: tasmoadmin
    image: "raymondmm/tasmoadmin:latest"
    restart: unless-stopped
    volumes:
      - /opt/tasmota-admin:/data
      - /etc/localtime:/etc/localtime:ro
    ports:
      - "9090:80"
    #network_mode: "host"
    environment:
      - TZ=America/Sao_Paulo
